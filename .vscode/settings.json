{
  "eslint.validate":[
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#213220",
    "titleBar.activeBackground": "#2F462D",
    "titleBar.activeForeground": "#FAFCFA"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}